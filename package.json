{
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn run partytown && yarn run prisma:generate && next build",
    "partytown": "partytown copylib public/~partytown",
    "dev": "yarn run prisma:generate && next dev",
    "start": "next start",
    "lint": "next lint",
    "lintfix": "eslint --fix .",
    "sanity-start": "sanity start",
    "sanity-build": "santiy build",
    "sanity-deploy": "sanity deploy",
    "type-check": "tsc --noEmit",
    "db:seed": "prisma db seed",
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "prisma:generate:psql": "prisma generate",
    "prisma:generate:mongo": "prisma generate --schema prisma/mongo-schema.prisma",
    "prisma:generate": "yarn run prisma:generate:psql && yarn run prisma:generate:mongo",
    "prisma:push:psql": "prisma db push",
    "prisma:push:mongo": "prisma db push --schema prisma/mongo-schema.prisma",
    "prisma:push": "yarn run prisma:push:psql && yarn run prisma:push:mongo"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@builder.io/partytown": "^0.8.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.3.1",
    "@sanity/image-url": "^1.0.1",
    "@sanity/webhook": "^2.0.0",
    "@tailwindcss/typography": "^0.5.9",
    "classnames": "^2.3.1",
    "crypto-js": "^4.1.1",
    "googleapis": "48.0.0",
    "lodash": "^4.17.21",
    "motion": "^10.16.2",
    "next": "^13.4.7",
    "next-auth": "^4.22.1",
    "next-mdx-remote": "^4.1.0",
    "next-sanity": "^0.8.4",
    "next-themes": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "dev-preview",
    "sanity-plugin-markdown": "^3.0.0-v3-studio.0",
    "sharp": "^0.31.2",
    "styled-components": "^5.3.5",
    "swr": "1.3.0",
    "tailwindcss": "^3.3.2",
    "use-delayed-render": "^0.0.7"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/lodash": "^4.14.196",
    "@types/react": "^18.2.14",
    "autoprefixer": "10.4.8",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.7",
    "globby": "13.1.2",
    "postcss": "^8.4.16",
    "prettier": "^2.8.8",
    "prisma": "^5.3.1",
    "reading-time": "^1.5.0",
    "rehype": "12.0.1",
    "rehype-autolink-headings": "6.1.1",
    "rehype-code-titles": "1.1.0",
    "rehype-prism-plus": "^1.5.0",
    "rehype-slug": "5.0.1",
    "remark-gfm": "^3.0.1",
    "rss": "1.2.2",
    "typescript": "^4.8.2"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
